<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>net9.0-windows</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>AcrControls-1</PackageId>
    <Version>1.0.0</Version>
    <Authors>Arthur Cabral</Authors>
    <Company>ACR Software</Company>
    <Description>ACR Windows Forms Controls.
Custom controls for Windows Forms .NetCore
</Description>
    <PackageTags>winforms;forms;controls;acr</PackageTags>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Acr WinForms Controls</Title>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>

  </PropertyGroup>

</Project>
